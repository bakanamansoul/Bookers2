<h2>User info</h2>
<%= form_with model: @user do |f| %>
  <label for="inputname">Name</label>
  <%= f.text_filed :name, autofocus: true, id:"inqutName", placeholder:"名前"%>
  <label for="inqutImage">Image</label>
  <%= f.file_filed :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "Update User" %>
<% end  